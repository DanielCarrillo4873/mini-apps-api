{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "import/extensions": [2, "always"],
      "import/no-extraneous-dependencies": [
        "error", {"devDependencies": true}
      ],
      "no-underscore-dangle": "off"
    }
}
